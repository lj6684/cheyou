<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <package name="example" extends="struts-default">
    
        <action name="brand_*" class="org.cheyou.web.BrandAction" method="{1}">
            <result name="success">brand_list.jsp</result>
            <result name="input">brand_modify.jsp</result>
        </action>
        
        <action name="supply_*" class="org.cheyou.web.SupplyAction" method="{1}">
            <result name="success">supply_list.jsp</result>
            <result name="input">supply_modify.jsp</result>
        </action>
        
        <action name="style_*" class="org.cheyou.web.StyleAction" method="{1}">
            <result name="success">style_list.jsp</result>
            <result name="input">style_modify.jsp</result>
        </action>
        
        <action name="styleFilter_*" class="org.cheyou.web.StyleFilterAction" method="{1}">
            <result name="success">filter_list.jsp</result>
        </action>
        
        <action name="filter_*" class="org.cheyou.web.FilterAction" method="{1}">
            <result name="success">filter_list.jsp</result>
            <result name="input">filter_add.jsp</result>
        </action>
        
        <action name="styleSpark_*" class="org.cheyou.web.StyleSparkAction" method="{1}">
            <result name="success">spark_list.jsp</result>
        </action>
        
        <action name="spark_*" class="org.cheyou.web.SparkAction" method="{1}">
            <result name="success">spark_list.jsp</result>
            <result name="input">spark_add.jsp</result>
        </action>
        
        <action name="query" class="org.cheyou.web.QueryAction">
            <result name="success">index.jsp</result>
            <result name="input">index.jsp</result>
        </action>
    </package>
</struts>

